{
  "name": "medico/medico",
  "description": "An application to track your doctors visits",
  "homepage": "http://cakephp.org",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=5.4.16",
    "cakephp/cakephp": "~3.0",
    "mobiledetect/mobiledetectlib": "2.*",
    "cakephp/migrations": "~1.0",
    "cakephp/cakephp_bootstrap": "3.x",
    "cakephp/plugin-installer": "*"
  },
  "require-dev": {
    "psy/psysh": "@stable",
    "cakephp/debug_kit": "~3.0",
    "cakephp/bake": "~1.0",
    "cakephp/cakephp-codesniffer": "2.*",
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/gmponos/CakeTwitterBootstrap.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/gmponos/CakeMap.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/gmponos/cakephp-csvview.git"
    }
  ],
  "scripts": {
    "post-install-cmd": "App\\Console\\Installer::postInstall",
    "post-autoload-dump": "Cake\\Composer\\Installer\\PluginInstaller::postAutoloadDump"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
